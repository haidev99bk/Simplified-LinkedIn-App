{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "e2e:build-android:debug": "npx detox build --configuration android.emu.debug",
    "e2e:build-android": "detox build --configuration android.emu.release",
    "e2e:build-ios:debug": "detox build --configuration ios.sim.debug",
    "e2e:build-ios": "detox build --configuration ios.sim.release",
    "e2e:android": "detox test --configuration android.emu.release",
    "e2e:ios": "detox test --configuration ios.sim.release"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.5",
    "@react-navigation/native": "^6.1.4",
    "@react-navigation/native-stack": "^6.9.10",
    "@testing-library/react-native": "^11.5.2",
    "luxon": "^3.2.1",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-device-info": "^10.3.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.4.3",
    "detox": "^20.1.3",
    "eslint": "^8.19.0",
    "jest": "^29.4.3",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|@react-navigation)|react-navigation|react-native-vector-icons/)"
    ],
    "testPathIgnorePatterns": [
      "e2e/"
    ],
    "moduleNameMapper": {
      "^@react-native-async-storage/async-storage$": "<rootDir>/__mocks__/@react-native-async-storage/async-storage.js"
    }
  }
}
